mixin category-icon
    -
        var categoryList = [
            {title: "Property", src: "property", menu: ["Квартиры", "Дома", "Земля"]},
            {title: "Transport", src: "transport", menu: ["Легковые", "Грузовые", "Мотоциклы"]},
            {title: "Jobs", src: "jobs", menu: []},
            {title: "Business", src: "business", menu: []},
            {title: "Services", src: "services", menu: []},
            {title: "Electronics", src: "electronics", menu: []},
            {title: "DIY & Tools", src: "tools", menu: ["Клеи и герметики","Шкафы и оборудование для шкафов для шкафов для шкафов"]},

            {title: "Child's world", src: "childs", menu: []},
            {title: "Antiques and collectables", src: "antiques", menu: []},
            {title: "Sport and active leisure", src: "sport", menu: []},
            {title: "Handmade Products", src: "handmade", menu: []},
            {title: "Courses and education", src: "courses", menu: []},
            {title: "Things to do", src: "things", menu: []},
            {title: "Community", src: "community", menu: []},
            {title: "Freebies", src: "freebies", menu: []},
        ];

    for item in categoryList
        li(class = "list__item")
             a(class="list__item--link" href="#")
